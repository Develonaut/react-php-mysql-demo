  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false,
    "source.fixAll": true,
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
  ],
  "[javascript]": {
